{
  "defaultBranch": "master",
  "contribBranch": "dev",
  "repository": {
    "url": "https://github.com/${repoKey}.git"
  },
  "branches": {
    "dev": {
      "protection": "$dev",
      "create": true,
      "upstream": "stage"
    },
    "stage": {
      "protection": "$stage",
      "create": true,
      "upstream": "master"
    },
    "master": {
      "protection": "$master",
      "create": true
    },
    "fix/*": {
      "upstream": "dev"
    },
    "feat/*": {
      "upstream": "dev"
    },
    "greenkeeper/*": {
      "upstream": "dev"
    }
  },
  "protection": {
    "$dev": {
      "@": "$full",
      "required_status_checks": {
        "strict": true,
        "contexts": []
      },
      "restrictions": {
        "users": "${mergeDevUsers}"
      }
    },
    "$stage": {
      "@": "$full",
      "required_status_checks": {
        "contexts": [
          "ci/circleci: deploy-local",
          "ci/circleci: deploy-dev",
          "ci/circleci: test-node-v10",
          "ci/circleci: test-node-v8.10"
        ]
      }
    },
    "$master": {
      "@": "$full",
      "required_status_checks": {
        "contexts": [
          "ci/circleci: deploy-stage",
          "ci/circleci: test-node-v10",
          "ci/circleci: test-node-v8.10"
        ]
      }
    },
    "$full": {
      "required_status_checks": {
        "strict": false,
        "contexts": [
          "ci/circleci: test-node-v10",
          "ci/circleci: test-node-v8.10"
        ]
      },
      "enforce_admins": true,
      "required_pull_request_reviews": {
        "dismissal_restrictions": {
          "users": "${dismissalUsers}",
          "teams": []
        },
        "dismiss_stale_reviews": true,
        "require_code_owner_reviews": true,
        "required_approving_review_count": "${requiredReviewCount}"
      },
      "restrictions": {
        "users": "${releaseUsers}",
        "teams": []
      }
    }
  }
}
